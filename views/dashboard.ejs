            var nume_el = document.getElementById('sel_name');
            var nume = nume_el.options[nume_el.selectedIndex].text;

            var mesaj_el = document.getElementById('message');
            var mesaj = mesaj_el.value;

            var xhr = new XMLHttpRequest();
            xhr.open('POST', '/dashboard');
            xhr.setRequestHeader('Content-Type', 'application/json');

            xhr.onload = function () {
                document.getElementById('success').classList.remove('animate');
                document.getElementById('warning').classList.remove('animate');

                void document.getElementById('success').offsetWidth;
                void document.getElementById('warning').offsetWidth;

                if (xhr.responseText == 'success') {
                    document.getElementById('success').classList.add('animate');
                } else {
                    document.getElementById('warning').classList.add('animate');
                }
            };

            xhr.send(JSON.stringify({ clasa: clasa, nume: nume, mesaj: mesaj }));
